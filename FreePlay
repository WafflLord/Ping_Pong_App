public class FreePlay
{
    Player[] players;
    Player redPlayer;
    Player bluePlayer;
    public FreePlay()
    {
        Club clubMembers = new Club();
        this.players = clubMembers.getRandomPlayers(2);
        this.redPlayer = this.players[0];
        this.bluePlayer = this.players[1];
    }
    
    public String[] getPlayers(){
        String[] names = new String[players.length];
        for(int i = 0; i < players.length; i++){
            names[i] = this.players[i].returnName();
        }
        return names;
    }
}
